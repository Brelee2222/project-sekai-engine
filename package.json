{
  "name": "sonolus-pjsekai-engine",
  "version": "1.1.1",
  "description": "A recreation of Project Sekai: Colorful Stage! engine in Sonolus",
  "author": "NonSpicyBurrito",
  "repository": "github:NonSpicyBurrito/sonolus-pjsekai-engine",
  "license": "MIT",
  "keywords": [
    "Sonolus"
  ],
  "main": "dist/index.cjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "sonolus-cli --dev",
    "prebuild": "tsc -p . --noEmit && eslint --ext .mts ./src && eslint --ext .cts ./lib",
    "build": "tsc -p ./lib/tsconfig.json && sonolus-cli --build && node ./lib/build.mjs"
  },
  "dependencies": {
    "sonolus-core": "~7.2.0-beta.0"
  },
  "devDependencies": {
    "@types/node": "^16.18.39",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-organize-imports": "^3.2.3",
    "sonolus.js": "~9.1.0-beta.1",
    "typescript": "~5.1.6"
  }
}
